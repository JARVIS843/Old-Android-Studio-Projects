<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <LinearLayout
        android:id="@+id/unmovabletitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"

        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="100dp"
                android:text=""
                android:background="#6CB6F3 "
                android:scrollbars="vertical"
                android:verticalScrollbarPosition="defaultPosition"
                android:scrollbarStyle="outsideOverlay"

                android:textSize="20dp"

                android:id="@+id/differentdishes"
                />

            <TextView

                android:id="@+id/finaloutput"
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:layout_weight="1"
                android:background="#52ED5D"
                android:text="" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#eaff00"
                android:layout_alignBottom="@id/finaloutput"
                android:text=""
                android:textSize="15dp"
                android:id="@+id/totaloutput"
                />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:orientation="horizontal"
                android:layout_alignParentRight="true"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Order"
                    android:textColor="#F3FF00"
                    android:textSize="25dp"


                    android:background="#1400FF"
                    android:onClick="order"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Reset"
                    android:textSize="25dp"
                    android:background="#FF0000"


                    android:onClick="reset"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#f7faff"
                android:layout_marginTop="30dp"
                android:layout_alignParentRight="true"
                >

                <Button
                    android:id="@+id/sub"
                    android:layout_width="52dp"
                    android:layout_height="wrap_content"

                    android:text="+"
                    android:textSize="15dp"
                    android:onClick="sub"
                    />
                <TextView
                    android:id="@+id/displaynum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:layout_marginLeft="5dp"
                    android:textSize="30dp"
                    />
                <Button
                    android:id="@+id/add"
                    android:layout_width="52dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="-"
                    android:textSize="15dp"
                    android:onClick="add"
                    />

            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>





    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/otherstuff"
        android:background="#52ED5D"
        >

        <Button
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="70dp"
            android:onClick="location"
            android:text="Location"
            android:textSize="20dp" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="70dp"
            android:text="Dial"
            android:textSize="20dp"
            android:onClick="dial"
            />

    </LinearLayout>





    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout

                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="700dp"
                tools:context=".MainActivity"
                android:orientation="vertical"

                android:id="@+id/pg1"
                >





                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_marginTop="160dp"
                    >

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:onClick="chicken"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg1r1ln"
                            android:text="chicken"
                            android:textSize="25dp"
                            android:background="#DEDEDE"
                            android:layout_marginLeft="30dp"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="30$"
                            android:textSize="25dp"
                            android:layout_marginLeft="86dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg1r1ln"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/chicken"
                            android:layout_alignBaseline="@id/pg1r1ln"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="40dp"
                            />

                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="70dp"
                        android:onClick="beef"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg1r1rn"
                            android:text="beef"
                            android:textSize="25dp"

                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="15$"
                            android:textSize="25dp"
                            android:layout_marginLeft="50dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg1r1rn"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/beef"
                            android:layout_alignBaseline="@id/pg1r1rn"
                            android:layout_marginTop="25dp"

                            />
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5" android:onClick="rottenflesh">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"

                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg1r2ln"
                            android:text="rotten flesh"
                            android:textSize="25dp"
                            android:onClick="rottenflesh"
                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="50$"
                            android:textSize="25dp"
                            android:layout_marginLeft="125dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg1r2ln"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/rottenflesh"
                            android:layout_alignBaseline="@id/pg1r2ln"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="20dp"

                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:onClick="pork"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg1r2rn"
                            android:text="Raw Pork"
                            android:textSize="25dp"

                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="20$"
                            android:textSize="25dp"
                            android:layout_marginLeft="106dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg1r2rn"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/rawpork"
                            android:layout_alignBaseline="@id/pg1r2rn"
                            android:layout_marginTop="25dp"

                            />
                    </RelativeLayout>




                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" >

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="30dp"
                        android:onClick="mutton">

                        <TextView
                            android:id="@+id/pg1r3ln"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#DEDEDE"

                            android:text="mutton"
                            android:textSize="25dp"

                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_alignLeft="@id/pg1r3ln"
                            android:layout_marginLeft="80dp"
                            android:background="#F50A0A"
                            android:text="100$"
                            android:textSize="25dp" />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:layout_alignBaseline="@id/pg1r3ln"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="25dp"
                            android:src="@drawable/mutton"

                            />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:onClick="rabbit">

                        <TextView
                            android:id="@+id/pg1r3rn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#DEDEDE"
                            android:onClick="rabbit"
                            android:text="cooked rabbit"
                            android:textSize="25dp"

                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_alignLeft="@id/pg1r3rn"
                            android:layout_marginLeft="153dp"
                            android:background="#F50A0A"
                            android:text="20$"
                            android:textSize="25dp" />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:layout_alignBaseline="@id/pg1r3rn"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="25dp"
                            android:src="@drawable/rabbit" />
                    </RelativeLayout>
                </LinearLayout>




            </LinearLayout>





            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="750sp"
                android:layout_marginTop="700dp"
                android:orientation="vertical"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1" >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:layout_marginTop="30dp"
                        android:onClick="milk"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg2r1ln"
                            android:text="Milk"
                            android:textSize="25dp"
                            android:onClick="milk"
                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="1$"
                            android:textSize="25dp"
                            android:layout_marginLeft="50dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg2r1ln"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/milk"
                            android:layout_alignBaseline="@id/pg2r1ln"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="10dp"

                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="50dp"
                        android:layout_marginTop="30dp"
                        android:onClick="water"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg2r1rn"
                            android:text="water"
                            android:textSize="25dp"
                            android:onClick="water"
                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="0$"
                            android:textSize="25dp"
                            android:layout_marginLeft="65dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg2r1rn"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/waterposion"
                            android:layout_alignBaseline="@id/pg2r1rn"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="20dp"

                            />
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1" >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:onClick="strength"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg2r2ln"
                            android:text="strength"
                            android:textSize="25dp"
                            android:onClick="strength"
                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="20$"
                            android:textSize="25dp"
                            android:layout_marginLeft="90dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg2r2ln"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/strengthpotion"
                            android:layout_alignBaseline="@id/pg2r2ln"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="20dp"

                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:onClick="sugar"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg2r2rn"
                            android:text="sugar"
                            android:textSize="25dp"
                            android:onClick="sugar"
                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="10$"
                            android:textSize="25dp"
                            android:layout_marginLeft="67dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg2r2rn"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/sugar"
                            android:layout_alignBaseline="@id/pg2r2rn"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="20dp"

                            />
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1" >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:onClick="egg"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg2r3ln"
                            android:text="Egg"
                            android:textSize="25dp"
                            android:layout_marginLeft="30dp"
                            android:background="#DEDEDE"
                            android:onClick="egg"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="2$"
                            android:textSize="25dp"
                            android:layout_marginLeft="40dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg2r3ln"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/egg"
                            android:layout_alignBaseline="@id/pg2r3ln"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="20dp"

                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:onClick="eye"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pg2r3rn"
                            android:text="spider Eye"
                            android:textSize="25dp"
                            android:onClick="eye"
                            android:background="#DEDEDE"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="10$"
                            android:textSize="25dp"
                            android:layout_marginLeft="115dp"
                            android:background="#F50A0A"
                            android:layout_alignLeft="@id/pg2r3rn"
                            />

                        <ImageView
                            android:layout_width="91dp"
                            android:layout_height="83dp"
                            android:src="@drawable/spidereye"
                            android:layout_alignBaseline="@id/pg2r3rn"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="20dp"

                            />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>

    </ScrollView>
</RelativeLayout>


